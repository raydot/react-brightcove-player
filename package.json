{
  "name": "nano-create-react-app",
  "version": "0.1.0",
  "description": "Using a minimal CRA to test the Brightcove player",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html"
  },
  "license": "MIT",
  "dependencies": {
    "@brightcove/react-player-loader": "^1.3.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "serialize-to-js": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-preset-nano-react-app": "^0.1.0",
    "parcel-bundler": "^1.12.3"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragmaFrag": "React.Fragment"
        }
      ]
    ]
  }
}
